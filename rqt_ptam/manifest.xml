<package>
  <description brief="rqt_ptam">

     Remote PTAM plugin for rqt, based on rqt_image_view from Dirk Thomas, TU Darmstadt

  </description>
  <author>Markus Achtelik</author>
  <license>BSD </license>
  <review status="unreviewed" notes="under active development"/>
  <url>http://ros.org/wiki/ethzasl_ptam/rqt_ptam</url>
  <depend package="roscpp"/>
  <depend package="std_msgs"/>
  <depend package="ptam_com"/>
  <depend package="sensor_msgs"/>
  <depend package="image_transport"/>
  <depend package="dynamic_reconfigure"/>
  <depend package="cv_bridge"/>
  <depend package="rqt_gui"/>
  <depend package="rqt_gui_cpp"/>
  
  <export>
    <rqt_gui plugin="${prefix}/plugin.xml"/>
  </export>
</package>
